    "pseudocode": [
      "def numFactoredBinaryTrees(arr):",
      "    arr.sort()",
      "    n = len(arr)",
      "    dp = [1] * n",
      "    index = {x: i for i, x in enumerate(arr)}",
      "    MOD = 10**9 + 7",
      "    for i in range(n):",
      "        for j in range(i):",
      "            if arr[i] % arr[j] == 0:",
      "                right = arr[i] // arr[j]",
      "                if right in index:",
      "                    k = index[right]",
      "                    dp[i] = (dp[i] + dp[j] * dp[k]) % MOD",
      "    return sum(dp) % MOD"
    ],
